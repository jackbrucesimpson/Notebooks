   "source": [
    "# Python Decorators"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "a wrapped up box of a new gpu\n"
     ]
    }
   ],
   "source": [
    "def add_wrapping(item):\n",
    "    def wrapped_item():\n",
    "        return 'a wrapped up box of a {}'.format(str(item()))\n",
    "    return wrapped_item\n",
    "\n",
    "@add_wrapping\n",
    "def new_gpu():\n",
    "    return 'new gpu'\n",
    "\n",
    "print(new_gpu())"
   ]
